{
  "manifest_version": 2,
  "name": "Dark Theme Generator",
  "description": "Makes webpages dark themed or generates a css as a bootstrap",
  "version": "0.1.0",

  "background": {
    "scripts": ["chrome.js", "background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["content-script.js"]
    }
  ],
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "permissions": [
    "notifications",
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
